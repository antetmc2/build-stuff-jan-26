{
  "name": "build-stuff-jan-26",
  "version": "0.0.0",
  "description": "oclif example Hello World CLI",
  "author": "Ante Tomic",
  "bin": {
    "build-stuff-jan-26": "./bin/run"
  },
  "homepage": "https://github.com/antetmc2/build-stuff-jan-26/",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:antetmc2/build-stuff-jan-26.git"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/command": "^1.8.21",
    "@oclif/core": "^1.26.1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.4.6",
    "edit-json-file": "^1.7.0",
    "lodash": "^4.17.21",
    "semver": "^7.3.8",
    "shelljs": "^0.8.5",
    "shx": "^0.3.4"
  },
  "devDependencies": {
    "@oclif/test": "^2.3.3",
    "@types/chai": "^4",
    "@types/edit-json-file": "^1.7.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.11",
    "@types/semver": "^7.3.13",
    "@types/shelljs": "^0.8.11",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "chai": "^4",
    "eslint": "^8.32.0",
    "eslint-config-oclif": "^4.0.0",
    "eslint-config-oclif-typescript": "^1.0.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-formatter-codeframe": "^7.32.1",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unicorn": "^45.0.2",
    "mocha": "^9",
    "oclif": "^3",
    "prettier": "^2.5.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "4.6.2"
  },
  "oclif": {
    "bin": "build-stuff-jan-26",
    "dirname": "build-stuff-jan-26",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts -f codeframe",
    "lint:fix": "npm run lint -- --fix",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "bugs": "https://github.com/antetmc2/build-stuff-jan-26/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
